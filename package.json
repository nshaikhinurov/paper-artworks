{
	"name": "paper-artworks",
	"version": "0.1.0",
	"description": "Paper.js-powered generative art",
	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack-dev-server --open",
		"webpack": "cross-env NODE_ENV=production webpack --progress --config webpack.config.js"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,json,jsx}": [
			"prettier --write",
			"git add "
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.4.3",
		"@babel/node": "^7.2.2",
		"@babel/preset-env": "^7.4.3",
		"@babel/register": "^7.0.0",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.5",
		"cross-env": "^5.2.0",
		"eslint": "^6.1.0",
		"eslint-config-airbnb": "^17.1.1",
		"eslint-config-prettier": "^6.0.0",
		"eslint-import-resolver-webpack": "^0.11.0",
		"eslint-plugin-import": "^2.18.2",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^1.3.1",
		"json-loader": "^0.5.7",
		"lint-staged": "^8.1.3",
		"prettier": "^1.17.1",
		"webpack": "^4.42.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3"
	},
	"dependencies": {
		"@babel/polyfill": "^7.0.0",
		"core-js": "^3.4.0",
		"paper": "^0.12.4",
		"ramda": "^0.26.1",
		"ramda-adjunct": "^2.22.1"
	}
}
